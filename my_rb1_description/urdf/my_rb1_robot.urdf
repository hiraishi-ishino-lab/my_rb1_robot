<?xml version="1.0"?>
<robot name="my_robot">
    <material name="green">
        <color rgba="0 0.5 0 1" />
    </material>

    <material name="blue_transparent">
        <color rgba="0 0 0.5 0.5" />
    </material>

    <material name="blue">
        <color rgba="0 0 0.5 1" />
    </material>
  <!-- Define the base footprint as the root link -->
  <link name="base_footprint"/>

  <!-- Define the base link -->
  <link name="base_link">
    <!-- Visual and collision properties for the base cylinder -->
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.25"/>
      </geometry>
       <material name="blue_transparent" />
       <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.3" radius="0.25"/>
      </geometry>
    </collision>
    <!-- Inertia properties for the base -->
    <inertial>
      <mass value="25"/>
      <inertia ixx="0.5781" ixy="0" ixz="0" iyy="0.5781" iyz="0" izz="0.7813"/>
    </inertial>
  </link>

  <!-- Define the right wheel -->
  <link name="right_wheel">
    <!-- Visual and collision properties for the wheel -->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.025"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5707 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.025"/>
      </geometry>
    </collision>
    <!-- Inertia properties for the wheel -->
    <inertial>
      <mass value="12.5"/>
      <inertia ixx="0.004557" ixy="0" ixz="0" iyy="0.004557" iyz="0" izz="0.003906"/>
    </inertial>
  </link>

  <!-- Define the left wheel -->
  <link name="left_wheel">
    <!-- Visual and collision properties for the wheel -->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.025"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5707 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.025"/>
      </geometry>
    </collision>
    <!-- Inertia properties for the wheel -->
    <inertial>
      <mass value="12.5"/>
      <inertia ixx="0.004557" ixy="0" ixz="0" iyy="0.004557" iyz="0" izz="0.003906"/>
    </inertial>
  </link>

  <!-- Define the front caster -->
  <link name="front_caster">
    <!-- Visual and collision properties for the caster -->
    <visual>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
       <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
    <!-- Inertia properties for the caster -->
    <inertial>
      <mass value="6.25"/>
      <inertia ixx="0.002604" ixy="0" ixz="0" iyy="0.002604" iyz="0" izz="0.002604"/>
    </inertial>
  </link>

  <!-- Define the back caster -->
  <link name="back_caster">
    <!-- Visual and collision properties for the caster -->
    <visual>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
       <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
    <!-- Inertia properties for the caster -->
    <inertial>
      <mass value="6.25"/>
      <inertia ixx="0.002604" ixy="0" ixz="0" iyy="0.002604" iyz="0" izz="0.002604"/>
    </inertial>
  </link>

  <!-- Define the front laser -->
  <link name="front_laser">
    <!-- Visual and collision properties for the laser -->
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.025"/>
      </geometry>
      <origin xyz="0 0 0" rpy="3.14159265359 0 0"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.15" radius="0.025"/>
      </geometry>
      <origin xyz="0 0 0" rpy="3.14159265359 0 0"/>
    </collision>
    <!-- Inertia properties for the laser -->
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00002"/>
    </inertial>
  </link>

  <!-- Define joints -->
  <joint name="base_footprint_to_base_link" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="base_to_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0 -0.2 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="base_to_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0 0.2 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="base_to_front_caster" type="continuous">
    <parent link="base_link"/>
    <child link="front_caster"/>
    <origin xyz="0.1 0 0"/>
    <axis xyz="0 1 0"/>
    <joint_properties damping="1.0" friction="0" />
  </joint>

  <joint name="base_to_back_caster" type="continuous">
    <parent link="base_link"/>
    <child link="back_caster"/>
    <origin xyz="-0.1 0 0"/>
    <axis xyz="0 1 0"/>
    <joint_properties damping="1.0" friction="0" />
  </joint>

  <joint name="base_to_front_laser" type="continuous">
    <parent link="base_link"/>
    <child link="front_laser"/>
    <origin xyz="0.25 0 0.075"/>
    <axis xyz="0 0 1"/>
  </joint>

</robot>
